{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "react"
    ],
    "globals": {
        "it": false,
        "expect": false,
        "describe": false
    },
    "rules": {
        "import/no-extraneous-dependencies": [
            "warn"
        ]
    }
}